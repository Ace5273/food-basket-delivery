    "publish-to-hbhr": "git push hbhr master -f",
    "publish-to-tikvabasket": "git push tikvabasket master -f",
    "publish-to-hesed": "git push hesed master -f",
    "publish-to-chasdei": "git push chasdei master -f",
    "publish-to-good-people": "git push good-people master -f",
    "publish-to-ladyc": "git push ladyc master -f",
    "publish-to-kruvit-sansana": "git push kruvit-sansana master -f",
    "publish-to-kruvit": "git push kruvit master -f",
    "publish-to-cbhr-doners": "git push cbhr-doners master -f",
    "publish-to-chabadekron": "git push chabadekron -f",
    "copy-db": "heroku pg:copy hugmoms::DATABASE_URL hugmoms2::DATABASE_URL -a hugmoms2",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "server:execute": "node dist-server/app/server/server.js",
    "server:debug": "node --inspect --debug-brk dist-server/app/server/server.js",
    "RunSomething": "node --inspect --debug-brk dist-server/src/app/server/doSomething.js",
    "server-watch": "ts-node-dev --respawn --project tsconfig.server.json  src/app/server/server.ts",
    "server-build-and-run": "npm run server:build && npm run server:execute",
    "build:stats": "ng build  --stats-json",
    "bundle-report": "webpack-bundle-analyzer dist/stats.json"
  },
  "private": true,
