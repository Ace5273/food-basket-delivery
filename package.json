{
  "name": "hug-moms",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "gc": "ng generate component --spec false",
    "ng-serve": "ng serve",
    "ng-serve-noam-ip-10.0.0.6": "ng serve --host 10.0.0.6",
    "start": "npm run server:execute",
    "build": "npm run server:build && npm run ng-build",
    "ng-build": "ng build --prod",
    "server:build": "tsc -p tsconfig.server.json",
    "server:build-watch": "tsc -p tsconfig.server.json --watch",
    "publish-to-heroku-test": "git push heroku -f",
    "publish-to-nrun": "git push nrun master -f",
    "publish-to-plev": "git push plev master -f",
    "publish-to-salmichal": "git push salmichal master -f",
    "publish-to-hmey": "git push hmey master -f",
    "publish-to-yadl": "git push yadl master -f",
    "publish-to-cbdh": "git push cbdh master -f",
    "publish-to-cbhr": "git push cbhr master -f",
    "publish-to-cbey": "git push cbey master -f",
    "publish-to-atbh": "git push atbh master -f",
    "publish-to-tal-chaim": "git push tal-chaim master -f",
    "publish-to-dinale": "git push dinale master -f",
    "publish-to-dinale-kol-halev": "git push kol-halev master -f",
    "publish-to-krvtj": "git push krvtj master -f",
    "publish-to-merkaz-mazon": "git push mmaz -f",
    "publish-to-bodedim-hertzelia": "git push bodedim-hertzelia master -f",
    "publish-to-shorashim": "git push shorashim master -f",
